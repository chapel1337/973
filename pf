-- originally made by [unknown]
-- edited by chapel1337

local windows = loadstring(game:HttpGet("https://raw.githubusercontent.com/ZepsyyCodesLUA/Synapse-Library-OBFUSCATED-/main/Source.lua"))()
local win = windows:Create({
    Title = "973",
    Game = "phantom forces"
})

local tab1 = win:NewTab({
    Title = "aimbot"
})

local tab2 = win:NewTab({
    Title = "esp"
})

local tab3 = win:NewTab({
    Title = "triggerbot"
})

local tab4 = win:NewTab({
    Title = "unlock all"
})

getgenv().aimbot_loaded = false
getgenv().esp_loaded = false
getgenv().triggerbot_loaded = false
getgenv().unlockall_loaded = false

tab1:Toggle({
    Title = "enable aimbot",
    Description = "basic aimbot",
    Callback = function(args)
        if not getgenv().TriggerBot_loaded then
            loadstring(game:HttpGet("https://raw.githubusercontent.com/skatbr/Roblox-Releases/main/PH_AIMBOT.lua", true))()
        end
        getgenv().aimbot_loaded = true
    end
})

tab1:DropDown({
    Text = "aimlock method",
    Options = {"head", "torso","random"},
    Callback = function(args)
        if args == "random" then
            getgenv().random_aim = true
        else
            getgenv().random_aim = false
            getgenv().aim_at = args
        end
    end
})


tab1:Slider({
    Title = "smoothing",
    MinValue = 1,
    Def = 2,
    MaxValue = 25,
    callback = function(args)
        getgenv().aim_smooth = args
    end
})

tab2:Toggle({
    Title = "enable aimbot",
    Description = "basic esp",
    Callback = function(args)
        if not getgenv().TriggerBot_loaded then
            loadstring(game:HttpGet("https://raw.githubusercontent.com/skatbr/Roblox-Releases/main/Phantom_%20Forces_ESP.lua", true))()
        end
        getgenv().esp_loaded = true
    end
})

tab3:Toggle({
    Title = "enable triggerbot",
    Description = "automatically shoots when cursor is placed over an enemy.",
    Callback = function(args)
        if not getgenv().triggerbot_loaded then
            loadstring(game:HttpGet("https://raw.githubusercontent.com/skatbr/Roblox-Releases/main/Phantom_%20Forces_TRIGGERBOT.lua", true))()
        end
        getgenv().triggerBot = args
    end
})

tab3:Toggle({
    Title = "headshots only",
    Description = "automatically shoots when cursor is placed over an enemy's head.",
    Callback = function(args)
        getgenv().head_check = args
    end
})

tab3:Slider({
    Title = "release time",
    MinValue = 0,
    Def = 0,
    MaxValue = 6,
    callback = function(args)
        getgenv().releasetime = args
    end
})

tab3:Slider({
    Title = "reaction time",
    MinValue = 0,
    Def = 0,
    MaxValue = 4,
    callback = function(args)
        getgenv().reaction_time = args
    end
})

tab4:Button({
    Title = "run",
    Callback = function()
        if not getgenv().unlockall_loaded then
            loadstring(game:HttpGet("https://pastebin.com/raw/LaDWm8M7", true))()
            getgenv().unlockall_loaded = true
        end

    end
})
